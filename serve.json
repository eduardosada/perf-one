{
  "headers": [
    {
      "source": "**/*.*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store"
        }
      ]
    }
  ],
  "unlisted": [
    ".DS_Store",
    ".git",
    "node_modules",
    "package.json",
    "package-lock.json",
    ".editorconfig",
    "serve.json"
  ]
}
